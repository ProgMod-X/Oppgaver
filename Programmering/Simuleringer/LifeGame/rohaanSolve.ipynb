    "import random\n",
    "\n",
    "def populate(width, height):\n",
    "    world = []\n",
    "    for i in range(height):\n",
    "        a = []\n",
    "        for j in range(width):\n",
    "            a.append(random.randint(0, 1))\n",
    "        world.append(a)\n",
    "    \n",
    "    return world\n",
    "\n",
    "print(populate(5, 6))"
   ]
