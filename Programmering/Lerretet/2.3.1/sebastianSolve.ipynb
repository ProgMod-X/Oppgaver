    "from ipycanvas import Canvas\n",
    "\n",
    "canvas = Canvas(width = 600, height=600)\n",
    "a = 1\n",
    "b = 1\n",
    "c = 10\n",
    "d = 10\n",
    "for i in range(7):\n",
