   "source": [
    "import time\n",
    "\n",
    "sekunder = time.time()\n",
    "minutt = sekunder / 60\n",
    "timer = minutt  / 60\n",
    "dager = timer / 24\n",
    "aar = dager / 365.25\n",
    "\n",
    "print(aar)"
   ]
  }
