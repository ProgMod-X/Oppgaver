   "source": [
    "# Oppgave 2.3.1.1\n",
    "Lag en funksjon som tar en liste med tall som parameter og returnerer summen av tallene."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sum(liste):\n",
    "    sum = 0\n",
    "    for i in liste:\n",
    "        sum += i\n",
    "    return sum"
   ]
  }
